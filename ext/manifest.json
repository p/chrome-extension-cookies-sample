{
  "name": "Cookie Setter",
  "description": "Cookie setting sample extension",
  "version": "1.0",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [{
    "js": ["content.js"],
    "matches":["http://striker:8112/*"]
  }],
  "web_accessible_resources":["page.js", "zepto.js"],
  "permissions":[
    "cookies",
    "http://localhost:8192/*",
    "http://striker:8112/*"
  ],
  "manifest_version": 2
}
